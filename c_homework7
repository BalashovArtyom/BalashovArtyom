    for(int i=0; i<256; i++)
    {
        scanf("%255s", buffer);
        if(strcmp(buffer, "STOP") != 0)
        {
            str[i]=(char*) malloc((strlen(buffer)+1) * sizeof(char));
            strcpy(str[i], buffer);
            k++;
            len+=strlen(buffer);
        }
