#include <stdio.h>
int func1(int n){
    int k;
    if(n==1) 
        return 0;
    k=func1(n-1)+1;
    printf("%d ", k);
    return k;
}

float func2(float n){
    if(n==1)
        return 0;
    n=n/2;
    if(n>2){
        func2(n);
        printf("%f", n);
    }
}
    
int func34(int a, int b){
    int k;
    if(a>b){
        if(a==b)
            return a;
        printf("%d",a + func34(a-1,b));
    }
    else{
        if(a==b)
            return a;
        printf("%d ", a + func34(a+1,b));
    }
}
int main()
{
    //func1(n1);
    //func2(n2);
    func34(25,30);
}
