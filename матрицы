#include <stdio.h>
#include <stdlib.h>
void matrix(int n)
{
    int i,j;
    scanf("%d", &n);
    int arr[n][n];
    for(i=0;i<n;i++){
        for(j=0;j<n;j++)
            arr[i][j] = rand() % 100;
    }
    for(i=0;i<n;i++){
        for(j=0;j<n;j++)
            printf("%d ",arr[i][j]);
        printf("\n");
    }
}

void trid_matrix(int n)
{
    int i,j;
    scanf("%d",&n);
    int arr[n][n];
    for(i=0;i<n;i++){
        for(j=0;j<n;j++)
            arr[i][j] = 0;
    }
    for(i=0;i<n;i++){
        arr[i][i]=1+rand();
        arr[i][i+1]=1+rand();
        arr[i][i-1]=1+rand();
    }
    for(i=0;i<n;i++){
        for(j=0;j<n;j++)
            printf("%d ",arr[i][j]);
        printf("\n");
    }
}

int main()
{
    int n;
    scanf("%d", &n);
    int **a = malloc(n * sizeof(*a));
    for (int i=0; i<n; i++)
        a[i] = malloc(n*sizeof(int));
    matrix(n);
    trid_matrix(n);
    for (int i=0; i<n; i++)
       free(a[i]);
    free(a);
}
