#include <stdio.h>
int func1(int n){
    int k;
    if(n==1) 
        return 0;
    k=func1(n-1)+1;
    printf("%d ", k);
    return k;
}

int func2(int n){
    n=n/2;
    if(n==2)
        return 1;
    else if(n>2)
        func2(n);
    else
        return 0;
}
    
int func34(int a, int b){
    int k;
    if(a>b){
        if(a==b)
            return a;
        return a + func34(a-1,b);
    }
    else{
        if(a==b)
            return a;
        return a + func34(a+1,b);
    }
}

int func5(int n){
    if (n<10)
        return n;
    else
        return n%10 + func5(n/10);
}

int func6(int p){
    int i=2;
    if(p<2)
        printf("No");
    else if(p==2)
        printf("Yes");
    else if(p%2==0)
        printf("No");
    else if(i<p/2){
        i++;
        return func6(p);
    }
    else
        printf("Yes");
}

int main()
{
    func1(10);
    printf("\n");
    if(func2(196))
      printf("Yes");
    else
      printf("No");
    printf("\n");
    printf("%d",func34(10,5));
    printf("\n");
    printf("%d",func5(1010122298));
    printf("\n");
    func6(5);
}
