Задача 1)
#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i, j, n, k;
    scanf("%d", &n);
    int arr[n][n];
    int *a;
    a = malloc(n* sizeof(int*));
    for(i=0;i<n;i++){
        for(j=0;j<n;j++)
            arr[i][j] = rand() % 100;
    }
    for(i=0;i<n;i++){
        for(j=0;j<n;j++)
            printf("%d ",arr[i][j]);
        printf("\n");
    }
    free(a);
}
